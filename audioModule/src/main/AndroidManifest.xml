<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rick.tictactoe.audiomanaging">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rick.tictactoe.audiomanaging.AudioSettingsActivity"/>
        </activity>
        <activity android:name=".addAudioFile" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.rick.tictactoe.audiomanaging.MainActivity"/>
        </activity>
        <activity android:name=".AudioSettingsActivity" />
        <activity android:name=".ExportImportActivity"></activity>
        <provider
            android:name="com.example.rick.tictactoe.Data.AudioFilesProvider"
            android:authorities="com.example.rick.tictactoe.Data.AudioFilesProvider"
            android:exported="false"
            android:grantUriPermissions="true"/>

    </application>

</manifest>